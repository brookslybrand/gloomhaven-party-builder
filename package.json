{
  "private": true,
  "name": "remix-app-template",
  "description": "",
  "license": "",
  "scripts": {
    "watch:css": "NODE_ENV=development postcss ./app/styles/tailwind.css -o ./app/styles/app.css -w",
    "build:css": "NODE_ENV=production postcss ./app/styles/tailwind.css -o ./app/styles/app.css",
    "predev": "touch ./app/styles/app.css",
    "dev": "concurrently \"npm run watch:css\" \"npm run start\" \"remix dev\"",
    "prebuild": "npm run build:css",
    "build": "remix build",
    "start": "vercel dev"
  },
  "dependencies": {
    "@remix-run/react": "^0.17.5",
    "@remix-run/vercel": "^0.17.5",
    "@vercel/node": "^1.8.3",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router-dom": "^6.0.0-beta.0",
    "remix": "^0.17.5"
  },
  "devDependencies": {
    "@remix-run/dev": "^0.17.5",
    "@types/react": "^17.0.4",
    "@types/react-dom": "^17.0.4",
    "autoprefixer": "^10.2.6",
    "concurrently": "^6.2.0",
    "postcss": "^8.3.1",
    "postcss-cli": "^8.3.1",
    "tailwindcss": "^2.1.4",
    "typescript": "^4.1.2",
    "vercel": "^23.0.0"
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false
}
